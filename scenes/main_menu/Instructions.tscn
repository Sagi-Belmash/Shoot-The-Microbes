[connection signal="pressed" from="Right" to="." method="_next_instruction" binds= [1]]
[connection signal="pressed" from="InstructionButtons/RotatingButton" to="." method="_change_instruction" binds= [0]]
[connection signal="pressed" from="InstructionButtons/MovingButton" to="." method="_change_instruction" binds= [1]]
